{
  "name": "node-ts-blob-microservice",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run files && babel-node ./dist/app.bundle.js",
    "dev": "npm run files && npm-run-all --parallel build:dev watch",
    "dev:unbundled": "nodemon --exec babel-node ./bin/www.ts",
    "watch": "nodemon --exec babel-node ./dist/app.bundle.js",
    "build:dev": "webpack-cli --config webpack.config.cjs -w --progress --color",
    "build": "webpack --config webpack.config.cjs --progress --color",
    "docker:dev": "bash ./docker-build-run.sh",
    "docker:hub": "bash ./docker-build-push.sh",
    "azure": "bash ./azure-deploy.sh",
    "files": "chmod +x ./azure-deploy-data.sh && chmod +x ./azure-blob-upload.sh"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "axios": "^0.21.1",
    "azure-cli": "^0.10.20",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^5.0.0",
    "chalk": "^4.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^7.0.3",
    "express": "~4.16.1",
    "http": "0.0.1-security",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "uuid": "^8.3.2",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.11",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.5",
    "@types/uuid": "^8.3.0",
    "ts-loader": "^8.0.14",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "dotenv-webpack": "^7.0.3"
  }
}
